From 6133415cf067d656c40997e00eace06d07aeab34 Mon Sep 17 00:00:00 2001
From: Eyal Reizer <eyalr@ti.com>
Date: Mon, 16 May 2011 17:47:46 +0300
Subject: [PATCH] PSP04.02.00.07.sdk: activate wireless extensions interface used by iwconfig

Signed-off-by: Eyal Reizer <eyalr@ti.com>
---
 arch/arm/mach-omap2/Kconfig |    1 +
 1 files changed, 1 insertions(+), 0 deletions(-)

Index: kernel/linux-2.6.37-psp04.02.00.07/arch/arm/mach-omap2/Kconfig
===================================================================
--- kernel.orig/linux-2.6.37-psp04.02.00.07/arch/arm/mach-omap2/Kconfig	2013-05-14 09:59:42.459227479 -0600
+++ kernel/linux-2.6.37-psp04.02.00.07/arch/arm/mach-omap2/Kconfig	2013-05-14 10:00:53.259226980 -0600
@@ -35,6 +35,7 @@
 	select ARCH_HAS_OPP
 	select PM_OPP if PM
 	select WL12XX_PLATFORM_DATA
+	select WIRELESS_EXT
 
 config ARCH_OMAP4
 	bool "TI OMAP4"
